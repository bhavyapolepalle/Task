   "source": [
    "def count_unique(input_string):\n",
    "    unique_characters = set(input_string)\n",
    "    return len(unique_characters)\n",
    "user_input = input(\"Enter a string: \")\n",
    "\n",
    "result = count_unique(user_input)\n",
    "print(\"Number of unique characters:\", result)"
   ]
